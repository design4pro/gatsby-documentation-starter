{
    "name": "gatsby-starter-documentation",
    "description": "A starter for a documentation powered by Gatsby and Markdown",
    "version": "0.0.1",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "dependencies": {
        "gatsby": "^2.18.18",
        "gatsby-theme-documentation-material-ui": "^0.0.1",
        "prop-types": "^15.7.2",
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.2.0",
        "@commitlint/config-conventional": "^8.2.0",
        "@commitlint/prompt-cli": "^8.2.0",
        "husky": "^3.0.5",
        "lint-staged": "^9.2.5",
        "prettier": "^1.19.1"
    },
    "homepage": "https://github.com/design4pro/gatsby-starter-documentation#readme",
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "main": "n/a",
    "author": "Rafa≈Ç Wolak <r.wolak@design4.pro>",
    "bugs": {
        "url": "https://github.com/design4pro/gatsby-starter-documentation/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/design4pro/gatsby-starter-documentation.git"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.(ts|tsx)": [
            "npm run --silent lint:fix",
            "git add"
        ]
    },
    "optionalDependencies": {
        "gatsby-source-filesystem": "^2.1.43"
    }
}
